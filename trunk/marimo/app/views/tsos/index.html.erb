<table class="list tsos">
  <thead>
  <tr>
    <th><%= _'Tso|Path' %></th>
    <th></th>
<%-if logged_in? -%>
    <th></th>
    <th></th>
<%-end-%>
  </tr>
  </thead>

  <tbody>
<% for tso in @tsos %>
  <tr class="<%= cycle('odd', 'even') %>">
    <td><%=h tso.path %></td>
    <td><%= link_to _('Show'), tso %></td>
<%-if logged_in? -%>
    <td><%= link_to _('Edit'), edit_tso_path(tso) %></td>
    <td><%= link_to _('Destroy'), tso, :confirm => _('Are you sure?'), :method => :delete %></td>
<%-end-%>
  </tr>
<% end %>
  </tbody>
</table>

<%-if logged_in? -%>
<%= link_to 'New tso', new_tso_path %>
<%-end-%>
<%= will_paginate(@tsos) %>
